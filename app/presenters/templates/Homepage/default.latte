{block content}
<div class="container">

  <div class="text-center">
  	<svg id="editor-area" width="1200px" height="700px">
      <circle cx="{$point[0]}" cy="{$point[1]}" r="3" stroke="black" stroke-width="1" fill="red" n:foreach="$points as $point"/>
      <text x="{$point[0] - 3}" y="{$point[1] - 7}" fill="#000" n:foreach="$points as $point">{$point[0]};{$point[1]}</text>
    </svg>
  </div>

</div><!-- /.container -->
